{
    "name": "dantleech/fink",
    "description": "Checks Links",
    "license": "MIT",
    "type": "library",
    "bin": [ "bin/fink" ],
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "amphp/dns": "^v1.2.2",
        "amphp/file": "^1",
        "amphp/http-client": "^4.1.0",
        "amphp/http-client-cookies": "^1",
        "league/uri": "^6",
        "phpactor/console-extension": "~0.1",
        "phpactor/container": "^1.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "amphp/phpunit-util": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpactor/test-utils": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DTL\\Extension\\Fink\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DTL\\Extension\\Fink\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "integrate": [
            "./vendor/bin/php-cs-fixer fix",
            "./vendor/bin/phpstan analyse",
            "./vendor/bin/phpunit"
        ]
    }

}
